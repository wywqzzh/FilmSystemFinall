<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="beans">

<!--    映射文件的作用：-->
<!--        1）类到表的yingshe-->
<!--        2）属性到字段的映射-->
    <class name="Seat" table="seats">



        <composite-id>

            <key-property name="arrangeId" column="arrangeId" type="java.lang.String"></key-property>
            <key-property name="col" column="seatCol" type="java.lang.Integer"></key-property>
            <key-property name="row" column="seatRow" type="java.lang.Integer"></key-property>
        </composite-id>
    </class>

</hibernate-mapping>